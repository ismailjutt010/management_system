{
  "AWSEBDockerrunVersion": 1,
  "containerDefinitions": [
    {
      "name": "ismailcode-api",
      "image": "ismail010/ismailcode-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://postgres:5432/customer"
        }
      ]
    }
  ]
}